{
  "name": "my-webpack-project",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config webpack.config.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "analyz": "cross-env NODE_ENV=production npm_config_report=true yarn build",
    "lint": "npx eslint --no-eslintrc -c .eslintrc.js ./ ",
    "lint:fix": "npx eslint  --fix --no-eslintrc -c .eslintrc.js ./ ",
    "docs:clean": "rimraf docs/.vuepress/.temp docs/.vuepress/.cache docs/.vuepress/dist",
    "docs:dev": "vuepress dev docs --clean-cache --clean-temp ",
    "docs:build": "vuepress build docs --clean-cache --clean-temp ",
    "docs:serve": "anywhere -h localhost -d docs/.vuepress/dist",
    "release": "release-it",
    "new": "plop"
  },
  "keywords": [
    "webpack"
  ],
  "author": "duanqy <duanqy0818@163.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@dqyfp/eslint-config-base": "^0.0.2",
    "@dqyfp/prettier-config-base": "^0.0.2",
    "@dqyfp/stylelint-config-base": "^0.0.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@webpack-cli/init": "^1.1.3",
    "anywhere": "^1.5.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.19.0",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^4.3.8",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "plop": "^2.7.4",
    "postcss": "^8.2.5",
    "postcss-loader": "^5.0.0",
    "release-it": "^14.3.0",
    "sass-loader": "^11.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.15",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "vuepress": "^2.0.0-alpha.20",
    "webpack": "^5.21.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@vue/compiler-sfc": "^3.0.5",
    "vue": "^3.0.5",
    "vue-loader": "^16.1.2",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
